            <string>///Deal with jump

if global.game_state == "PLAYING"{
    key_jump = keyboard_check_pressed(vk_space);
    key_jump_held = keyboard_check(vk_space);
    //Gravity
    if (vsp &lt; g_max) vsp += g;
    //React to the jump input
    if (place_meeting(x,y+1,o_floor)){
        vsp = key_jump * - jump_speed;
    }
    //Vertical collision + move
    if (place_meeting(x,y+vsp,o_floor)){
        while(!place_meeting(x,y+1,o_floor)){
            y++;
        }
        vsp = 0;
    }
    
    if (vsp &lt; 0) and (!key_jump_held) vsp = max(vsp,-3);
    
    y += vsp;
}

</string>
