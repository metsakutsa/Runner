            <string>///Check for system keys

//Pause toggle
if keyboard_check_pressed(vk_escape) and global.game_state == "PLAYING"{
    state_change("PAUSED");
    //Pause acceleration
    with o_floor_spawner{
        o_floor_spawner.hold_alarm = alarm[0];
        o_floor_spawner.alarm[0] = 0;
    }
    
    //Pause obstacle spawner
    with o_obstacle_spawner{
        o_obstacle_spawner.hold_alarm = alarm[0];
        o_obstacle_spawner.alarm[0] = 0;
    }
    keyboard_clear(vk_escape)
}
if keyboard_check_pressed(vk_escape) and global.game_state == "PAUSED"{
    state_change("PLAYING");
    //Continue acceleration
    with o_floor_spawner{
        if hold_alarm &gt; 0 {
            alarm[0] = hold_alarm;
            hold_alarm = 0;
        } else {
            alarm[0] = acceleration;
        }
    }
    
    //Continue obstacle spawner
    with o_obstacle_spawner{
        if hold_alarm &gt; 0 {
            alarm[0] = hold_alarm;
            hold_alarm = 0;
        } else {
            alarm[0] = acceleration;
        }
    }
}

//Restart when game over
if keyboard_check_pressed(ord("R")) and global.game_state == "GAMEOVER"{
    room_restart();
    state_change("PLAYING");
}
</string>
